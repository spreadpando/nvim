{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "disabled": false
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx",
      "disabled": false
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "vectorcode": {
      "autoApprove": [
        "ls",
        "query"
      ],
      "command": "vectorcode-mcp-server",
      "disabled": false
    }
  }
}
